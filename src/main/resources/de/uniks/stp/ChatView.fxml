<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox id="root" fx:id="topVbox" prefHeight="2110.0" prefWidth="3469.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane id="stack" prefHeight="2074.0" prefWidth="200.0">
         <children>
            <VBox alignment="BOTTOM_RIGHT" prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: transparent;">
               <children>
                  <HBox alignment="BOTTOM_RIGHT" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: transparent;">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="100.0" style="-fx-background-color: transparent;" />
                        <fx:include source="emojiList.fxml" />
                     </children>
                  </HBox>
               </children></VBox>
            <VBox id="container" prefHeight="200.0" prefWidth="400.0">
               <children>
                  <ListView fx:id="messageListView" prefHeight="200.0" prefWidth="390.0" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </children>
      </StackPane>
      <HBox id="container" alignment="CENTER_LEFT" prefHeight="1.0" prefWidth="3469.0" />
      <HBox id="messageBox" alignment="CENTER" maxHeight="35.0" prefHeight="35.0" prefWidth="400.0" spacing="20.0">
         <children>
            <HBox prefHeight="35.0" prefWidth="4.0" />
            <TextField fx:id="messageTextField" style="-fx-background-radius: 6; -fx-border-radius: 6;" />
            <Button id="emojiButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="30.0" style="-fx-background-radius: 20;">
               <graphic>
                  <ImageView id="imageView" fitHeight="19.0" fitWidth="29.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@grinning-face.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button id="sendButton" fx:id="sendButton" mnemonicParsing="false" text="%button.send" textFill="WHITE" />
            <HBox prefHeight="35.0" prefWidth="4.0" />
         </children>
      </HBox>
   </children>
</VBox>
