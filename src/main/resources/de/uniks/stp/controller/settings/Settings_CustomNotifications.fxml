<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="161.0" prefWidth="196.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label id="selectedSound" alignment="CENTER" prefHeight="20.0" prefWidth="130.0" text="Selected Sound:" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <ComboBox id="comboBox" prefHeight="25.0" prefWidth="140.0" promptText="saved sounds" />
      <HBox alignment="CENTER" prefHeight="30.0" prefWidth="100.0" spacing="20.0">
         <children>
            <Button id="add" mnemonicParsing="false" prefWidth="60.0" style="-fx-background-radius: 10;" text="Add" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button id="delete" mnemonicParsing="false" prefWidth="60.0" style="-fx-background-radius: 10;" text="Delete" textFill="RED">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
         </children>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
